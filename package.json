{
  "name": "cratepilot",
  "version": "1.0.0",
  "description": "AI-powered DJ crate planning tool with Gemini integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "npx ts-node tests/run_all_tests.ts",
    "test:catalog": "npx ts-node tests/catalog.test.ts",
    "test:scorer": "npx ts-node tests/scorer.test.ts",
    "test:validator": "npx ts-node tests/validator.test.ts",
    "test:planner": "npx ts-node tests/crate_planner.test.ts",
    "test:manual": "npx ts-node -e \"require('./tests/crate_planner.test.ts').testManualCratePlanning()\"",
    "test:ai": "npx ts-node -e \"require('./tests/crate_planner.test.ts').testAICratePlanning()\"",
    "test:revision": "npx ts-node -e \"require('./tests/crate_planner.test.ts').testCrateRevision()\"",
    "test:mixed": "npx ts-node -e \"require('./tests/crate_planner.test.ts').testMixedModePlanning()\"",
    "test:spotify": "npx ts-node tests/spotify_importer.test.ts",
    "example:spotify": "npx ts-node examples/spotify_import_example.ts"
  },
  "keywords": ["dj", "music", "crate", "planning", "ai", "gemini", "rekordbox", "serato", "mixing", "typescript"],
  "author": "CratePilot",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  }
}
